{
  "manifest_version": 2,
  "name": "Tabbit",
  "version": "1.1.5",
  "description": "See the top image from a Reddit subreddit whenever you open a new tab.",
  "author": "Melody Kuo",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}